rgetmeta {
  name: Update Profile - No Token
  type: http
  seq: 3
}

put {
  url: {{userServiceUrl}}/api/users/profile
  body: json
  auth: none
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "id": "test_user_id",
    "email": "updated@example.com",
    "name": "Updated Name",
    "phone": "+9999999999",
    "role": "CUSTOMER"
  }
}

tests {
  test("Profile update fails without token", function() {
    expect(res.status).to.equal(401);
    expect(res.body).to.have.property('error');
  });
}
